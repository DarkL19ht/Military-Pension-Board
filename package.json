{
    "name": "mpb-admin-ui",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "start": "vite",
        "test": "vitest",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "lint:fix": "eslint --fix",
        "preview": "vite preview",
        "start:development": "vite --mode development --port 12000 --open",
        "start:staging": "vite --mode staging",
        "start:production": "vite  --port 12000 --open --mode production",
        "format": "prettier --write src",
        "prepare": "husky install",
        "svgr": "svgr --out-dir src/assets/icons --ignore-existing -- icons --replace-attr-values \"#000=currentColor\"",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "start:all": "concurrently \"npm run start:staging\" \"npm run storybook\""
    },
    "dependencies": {
